{
  "name": "stockwise",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "lint": "eslint . --fix",
    "format": "prettier --config .prettierrc 'src/**/*.{js,ts}' --write",
    "build": "tsc",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:run:prod": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/infra/dataSourceProd.ts migration:run",
    "typeorm:revert:prod": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/infra/dataSourceProd.ts migration:revert",
    "typeorm:generate:prod": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/infra/dataSourceProd.ts migration:generate ./src/infra/db/migrations/default",
    "typeorm:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/infra/dataSourceLocal.ts migration:run",
    "typeorm:revert": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/infra/dataSourceLocal.ts migration:revert",
    "typeorm:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d ./src/infra/dataSourceLocal.ts migration:generate ./src/infra/db/migrations/default",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bunyan": "^1.8.11",
    "@types/bunyan-format": "^0.2.9",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.0.2",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yamljs": "^0.2.34",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.8.0",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^15.8.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.25.0",
    "bcrypt": "^5.1.1",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "helmet": "^7.1.0",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "stockwise": "file:",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "yamljs": "^0.3.0"
  },
  "_moduleAliases": {
    "@config": "dist/application/config",
    "@controller": "dist/application/controller",
    "@routes": "dist/application/routes",
    "@service": "dist/application/service",
    "@utils": "dist/application/utils",
    "@infra": "dist/infra",
    "@model": "dist/domain/model",
    "@repository": "dist/infra/repository",
    "@dto": "dist/domain/dto",
    "@tests": "dist/tests"
  }
}
